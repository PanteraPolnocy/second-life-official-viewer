      <text type="string" length="1" follows="left|top" height="20" layout="topleft" left="5" mouse_opaque="false" name="b_val_text_float" width="60">青：</text>
      <spinner decimal_digits="3" follows="left|top" height="20" increment="0.01" initial_value="0.5" layout="topleft" left_delta="63" max_val="1.0" name="bspin_float" top_delta="0" width="50" />
